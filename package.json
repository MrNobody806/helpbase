{
  "name": "helpbase",
  "version": "1.0.0",
  "description": "Customer support platform built with Next.js and Cloudflare Pages",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "preview": "next start",
    "dev:worker": "wrangler dev workers/auth.js",
    "dev:full": "concurrently \"npm run dev:worker\" \"npm run dev\"",
    "deploy:pages": "npm run build && wrangler pages publish out --project-name=helpbase",
    "deploy:worker": "wrangler deploy workers/auth.js"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@supabase/supabase-js": "^2.38.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "memo": "^1.0.1",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-slick": "^0.31.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.2.10",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar-hide": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "20.0.0",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "tailwindcss": "^4",
    "wrangler": "^4.43.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
