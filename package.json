{
  "name": "helpbase",
  "version": "1.0.0",
  "description": "Customer support platform built with Next.js and Cloudflare Pages",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "preview": "next start",
    "dev:worker": "wrangler dev workers/auth.js",
    "dev:full": "concurrently \"npm run dev:worker\" \"npm run dev\"",
    "deploy:pages": "npm run build && wrangler pages publish out --project-name=helpbase",
    "deploy:worker": "wrangler deploy workers/auth.js"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tailwindcss/postcss": "^4.1.14",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "18.0.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
